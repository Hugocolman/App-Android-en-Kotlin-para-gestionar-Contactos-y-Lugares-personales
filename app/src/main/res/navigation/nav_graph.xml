<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/contactsListFragment">

    <fragment
        android:id="@+id/contactsListFragment"
        android:name="com.example.mislugaresnavegacion.ContactsListFragment"
        android:label="fragment_contacts_list"
        tools:layout="@layout/fragment_contacts_list" >
        <action
            android:id="@+id/action_contactsListFragment_to_addEditContactFragment"
            app:destination="@id/addEditContactFragment" />
    </fragment>
    <fragment
        android:id="@+id/placesListFragment"
        android:name="com.example.mislugaresnavegacion.PlacesListFragment"
        android:label="fragment_places_list"
        tools:layout="@layout/fragment_places_list" />
    <fragment
        android:id="@+id/addEditContactFragment"
        android:name="com.example.mislugaresnavegacion.AddEditContactFragment"
        android:label="AddEditContactFragment" >
        <argument android:name="contactToEdit"
            app:argType="com.example.mislugaresnavegacion.Contact"
            app:nullable="true"
            android:defaultValue="@null" />
    </fragment>
</navigation>